{
    "name": "openconext/serviceregistry",
    "description": "Idp/Sp metadata registry",
    "minimum-stability": "dev",
    "require": {
        "doctrine/migrations": "dev-master as 1",
        "doctrine/doctrine-migrations-bundle": "dev-master as 1",
        "janus-ssp/janus": "dev-feature/rest-api",
        "robloach/component-installer": "0.0.11",
        "dbpatch/dbpatch": "*",
        "openid/php-openid": "dev-master#ee669c6a9d4d95b58ecd9b6945627276807694fb as 2.2.2",
        "simplesamlphp/simplesamlphp": "dev-master#cdf7cb95a8c9b682534f9169f3f5a27198f6bcc5 as 1.12.0-alpha",
        "janus-ssp/rest-api-bundle": "dev-master as 1",
        "fsc/hateoas-bundle": "dev-master#e36875985202c91eb52d4cc2923766b0b015539a as 0.5",
        "psr/log": "1.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/janus-ssp/ServiceRegistryRestApiBundle"
        },
        {
            "type": "package",
            "package": {
                "name": "robrichards/xmlseclibs",
                "version": "1.3.1",
                "source": {
                    "type": "svn",
                    "url": "http://xmlseclibs.googlecode.com/svn",
                    "reference": "trunk@50"
                },
                "dist": {
                    "url": "https://xmlseclibs.googlecode.com/files/xmlseclibs-1.3.1.tar.gz",
                    "type": "tar"
                },
                "autoload": {
                    "files": ["xmlseclibs.php"]
                }
            }
        }
    ],
    "autoload": {
        "classmap": [ "lib/"]
    },
    "extra": {
        "symfony-app-dir": "vendor/janus-ssp/janus/app",
        "symfony-web-dir": "vendor/janus-ssp/janus/web"
    },
    "scripts": {
        "post-install-cmd": [
            "bin/simplesamlphp-post-install.sh",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "bin/simplesamlphp-post-install.sh",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    }
}
